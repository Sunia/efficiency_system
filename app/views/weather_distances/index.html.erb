<p id="notice"><%= notice %></p>

<h1>Listing Weather Distances</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weather_distances.each do |weather_distance| %>
      <tr>
        <td><%= link_to 'Show', weather_distance %></td>
        <td><%= link_to 'Edit', edit_weather_distance_path(weather_distance) %></td>
        <td><%= link_to 'Destroy', weather_distance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weather distance', new_weather_distance_path %>
