<p id="notice"><%= notice %></p>

<h1>Listing Passage Plans</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @passage_plans.each do |passage_plan| %>
      <tr>
        <td><%= link_to 'Show', passage_plan %></td>
        <td><%= link_to 'Edit', edit_passage_plan_path(passage_plan) %></td>
        <td><%= link_to 'Destroy', passage_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Passage plan', new_passage_plan_path %>
